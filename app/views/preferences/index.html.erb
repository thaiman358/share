	<div class="fh5co-loader"></div>
	
	<div id="page">

	<header id="fh5co-header" class="fh5co-cover js-fullheight" role="banner" style="background-image: url(assets/hero_1.jpeg);" data-stellar-background-ratio="0.5">
		<div class="overlay"></div>
		<div class="container">
			<div class="row">
				<div class="col-md-12 text-center">
					<div class="display-t js-fullheight">
						<div class="display-tc js-fullheight animate-box" data-animate-effect="fadeIn">
							<h1>Preference</h1>
							<h2><%= link_to "Ask Client's Preference", new_preference_path %></h2>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>

	<div id="fh5co-blog" class="fh5co-section">
		<div class="container">
			<div class="row animate-box">
				<div class="col-md-8 col-md-offset-2 text-center fh5co-heading animate-box">
					<h2>Your Preference</h2>

					<% @preference.each do |preference|%>
						<p><%= preference.preference%></p>
						<p><%= link_to "edit", edit_preference_path(preference.id) %><p>
					<% end %>

				</div>
			</div>

			<div class="row animate-box">
				<div class="col-md-8 col-md-offset-2 text-center fh5co-heading animate-box">
					<h2>Past Client's Preference</h2>

					<% @otherpreference.each do |others|%>
						<h3><%= others.client_name%>様</h3>
						<p><%= others.preference%></p>
					<% end %>

				</div>
			</div>
			
			
		</div>
	</div>


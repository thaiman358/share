	<div id="fh5co-about" class="fh5co-section">
		<div class="container">
			<div class="row">
				<div class="col-md-4 col-md-push-1 animate-box">
					<div class="section-heading">
						<%= form_with(model: @user, local: true) do |f| %>
            <table>
              <tr>
                <td>
                  <%= f.label :name %>
                </td>
                <td>
                  <%= f.text_field :name %>
                </td>
              </tr>
              <br>
              <tr>
                <td>
                  <%= f.label :email %>
                </td>
                <td>
                  <%= f.email_field :email %>
                </td>
              </tr>
              <br>
              <tr>
                <td>
                  <%= f.label :password %>
                </td>
                <td>
                  <%= f.password_field :password %>
                </td>
              </tr>
              <br>
              <tr>
                <td>
                  <%= f.label :password_confirmation %>
                </td>
                <td>
                  <%= f.password_field :password_confirmation %>
                </td>
              </tr>
            </table>
						<p><a href="#" class="btn btn-primary btn-outline"><%= f.submit "Sign up!" %></a></p>
						
						<% end %>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="fh5co-loader"></div>
	
	<div id="page">

	<header id="fh5co-header" class="fh5co-cover js-fullheight" role="banner" style="background-image: url(assets/hero_1.jpeg);" data-stellar-background-ratio="0.5">
		<div class="overlay"></div>
		<div class="container">
			<div class="row">
				<div class="col-md-12 text-center">
					<div class="display-t js-fullheight">
						<div class="display-tc js-fullheight animate-box" data-animate-effect="fadeIn">
							<h1><em>My</em> Page</h1>

						</div>
					</div>
				</div>
			</div>
		</div>
	</header>

	<div id="fh5co-timeline">
		<div class="container">
			<div class="row">
				<div class="col-md-12 col-md-offset-0">
					<ul class="timeline animate-box">
						<li class="timeline-heading text-center animate-box">
							<div><h3><%= button_to "Logout", session_path(current_user.id), :method=>:delete, :class=>:destroy %></h3></div>
							<div><h3>about yourself</h3></div>
						</li>
						<li class="animate-box timeline-unverted">
							<div class="timeline-badge"><i class="icon-genius"></i></div>
							<div class="timeline-panel">
								<div class="timeline-heading">
									<h3 class="timeline-title">Handle Name</h3>
								
								</div>
								<div class="timeline-body">
									<p><%= @user.name%></p>
								</div>
							</div>
						</li>
						<li class="timeline-inverted animate-box">
							<div class="timeline-badge"><i class="icon-genius"></i></div>
							<div class="timeline-panel">
								<div class="timeline-heading">
									<h3 class="timeline-title">Email</h3>
								</div>
								<div class="timeline-body">
									<p><%= @user.email%></p>
								</div>
							</div>
						</li>
						<li class="animate-box timeline-unverted">
							<div class="timeline-badge"><i class="icon-genius"></i></div>
							<div class="timeline-panel">
								<div class="timeline-heading">
									<h3 class="timeline-title">Industry</h3>
								</div>
								<div class="timeline-body">
								    <% industries = ["商社","メーカー","小売","金融","インフラ","物流","建設・不動産","サービス業","広告・マスコミ","ソフトウエア・通信","公務員","その他"]%>
									<p>
									<%= industries[(@user.industry-1)]%>
									</p>
								</div>
							</div>
						</li>
						</li>
						<li class="timeline-inverted animate-box">
							<div class="timeline-badge"><i class="icon-genius"></i></div>
							<div class="timeline-panel">
								<div class="timeline-heading">
									<h3 class="timeline-title">Title</h3>
								</div>
								<div class="timeline-body">
									<% choices = ["取締役会長クラス","取締役クラス","執行役員クラス","部長クラス","課長クラス","係長クラス"]%>
									<p>
								      <%= choices[(@user.title-1)]%>
									</p>
								</div>
							</div>
						</li>
						<li class="animate-box timeline-unverted">
							<div class="timeline-badge"><i class="icon-genius"></i></div>
							<div class="timeline-panel">
								<div class="timeline-heading">
									<h3 class="timeline-title">Your Like</h3>
								</div>
								<div class="timeline-body">
									<p>~~~~</p>
								</div>
							</div>
						</li>

			    	</ul>
				</div>
			</div>
		</div>
	</div>